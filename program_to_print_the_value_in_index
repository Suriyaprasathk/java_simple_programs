import java.util.Scanner;
public class Kthelement{
public static int keyvalue(int arr[],int key){
return arr[key];
}
public static void main(String args[]){
Scanner a=new Scanner(System.in);
System.out.println("enter the size of an array");
int size=a.nextInt();
int arr[]=new int[size];
System.out.println("enter the elements to add in the array");
for(int i=0;i<size;i++){
int x=a.nextInt();
arr[i]=x;
}
System.out.println("element in the array");
for(int i=0;i<size;i++){
System.out.println(arr[i]);
}
System.out.println("enter the index value");
int index=a.nextInt();
System.out.println("the element present in the index "+index+" is "+keyvalue(arr,index));
}
}
